<section>
    <div class="container mt-5">
        <table class="table mt-5">
          <thead>
            <tr>
              <th scope="col">Name</th>
              <th scope="col">Email</th>
            </tr>
          </thead>
          <tbody>
            {{#each users}}
            <tr>
              <td>{{this.Name}}</td>
              <td>{{this.Email}}</td>
            </tr>
            {{/each}}
          </tbody>
        </table>
    </div>
</section>